<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="@dimen/main_usage_circle_width"
    android:layout_height="@dimen/main_usage_circle_height"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_centerVertical="true"
    android:layout_centerHorizontal="true"
    android:id="@+id/relativeLayout">

    <com.example.dina_el_hakim.vodafonecircularviewtest.VodafoneUsageCircle
        android:id="@+id/main_usage_circle"
        style="@style/VodafoneUsageCircle.Compact"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:layout_marginRight="@dimen/usage_circle_margin"
        android:layout_gravity="center"
        app:vuc_fillColor="@color/white"
        app:vuc_backgroundColor="@color/white_50_percent_opacity"
        app:vuc_strokeWidth="@dimen/usage_circle_stroke_normal"
        app:vuc_fillAmount="1" />
</RelativeLayout>